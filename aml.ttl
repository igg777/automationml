aml:hasRoleClass
      rdf:type owl:ObjectProperty ;
      rdfs:comment "A Role Class lib contains a Role Class"^^xsd:string ;
      rdfs:domain aml:RoleClassLib ;
      rdfs:label "has Role Class"^^xsd:string ;
      rdfs:range aml:RoleClass .

aml:hasRoleClassLib
      rdf:type owl:ObjectProperty ;
